{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/generate",
            "destination": "/api/generate.js"
        },
        {
            "source": "/(.*)",
            "destination": "/$1"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=0, must-revalidate"
                }
            ]
        }
    ]
}